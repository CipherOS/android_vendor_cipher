<?xml version='1.0' encoding='utf-8' standalone='yes' ?>
<carrier_config_list>
    <carrier_config>

        <!-- Enable VoLTE and VoWiFi by default -->
        <boolean name="carrier_volte_available_bool" value="true" />
        <boolean name="carrier_wfc_ims_available_bool" value="true" />

        <!-- Config to show LTE+ icon if available -->
        <boolean name="hide_lte_plus_data_icon_bool" value="false"/>

        <!-- Should "4G" be shown instead of "LTE" when the network is NETWORK_TYPE_LTE? -->
        <boolean name="show_4g_for_lte_data_icon_bool" value="true"/>

        <int name="lte_plus_threshold_bandwidth_khz_int" value="0"/>

        <!-- Enable VoNR by default on supported devices -->
        <boolean name="vonr_enabled_bool" value="true"/>

        <int name="nr_advanced_threshold_bandwidth_khz_int" value="0"/>

    </carrier_config>
</carrier_config_list>
